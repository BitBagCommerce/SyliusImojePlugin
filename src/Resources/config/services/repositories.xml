<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\SyliusIngPlugin\Repository\PaymentMethodRepository"
                 id="bitbag.sylius_ing_plugin.repository.payment_method_repository">
            <argument type="service" id="sylius.repository.payment_method"/>
        </service>

        <service class="BitBag\SyliusIngPlugin\Repository\Order\OrderRepository"
                 id="bitbag.sylius_ing_plugin.repository.order.order_repository"
        >
            <argument type="service" id="sylius.repository.order"/>
        </service>
    </services>
</container>
